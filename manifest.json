{
    "manifest_version": 3,
    "name": "Impulse",
    "version": "1.0.0",
    "description": "An app to reduce impulsive spending",
    "permissions": [
        "storage",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "src/frontend/background.js", 
        "type": "module"
    }
}